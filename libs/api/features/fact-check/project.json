{
    "name": "api-feature-fact-check",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/api/features/fact-check/src",
    "prefix": "api",
    "tags": [
        "type:feature",
        "scope:api"
    ],
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/api/features/fact-check/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/api/features/fact-check/jest.config.ts",
                "passWithNoTests": true
            }
        }
    }
}