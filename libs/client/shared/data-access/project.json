{
  "name": "data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/shared/data-access/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "type:data-access",
    "scope:client-shared"
  ],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/client/shared/data-access/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/client/shared/data-access/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/client/shared/data-access/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/client/shared/data-access/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}