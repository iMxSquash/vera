{
  "name": "client-shared-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/shared/ui/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/client/shared/ui/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/client/shared/ui/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/client/shared/ui/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/client/shared/ui/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/client/shared/ui/.storybook",
        "browserTarget": "client-shared-ui:build-storybook",
        "compodoc": false,
        "styles": [
          "libs/client/shared/ui/.storybook/storybook.scss"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "outputDir": "dist/storybook/ui",
        "configDir": "libs/client/shared/ui/.storybook",
        "browserTarget": "client-shared-ui:build-storybook",
        "compodoc": false,
        "styles": [
          "libs/client/shared/ui/.storybook/storybook.scss"
        ]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}