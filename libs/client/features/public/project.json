{
    "name": "client-feature-public",
    "$schema": "../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/client/features/public/src",
    "prefix": "app",
    "targets": {
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/client/features/public/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/client/features/public/**/*.ts",
                    "libs/client/features/public/**/*.html"
                ]
            }
        }
    },
    "tags": [
        "type:feature",
        "scope:client"
    ]
}