{
    "name": "client-feature-auth",
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "projectType": "library",
    "sourceRoot": "libs/client/features/auth/src",
    "prefix": "app",
    "tags": [
        "type:feature",
        "scope:web"
    ],
    "targets": {
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/client/features/auth/**/*.ts",
                    "libs/client/features/auth/**/*.html"
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/client/features/auth/jest.config.ts",
                "passWithNoTests": true
            }
        }
    }
}